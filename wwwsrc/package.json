{
  "name": "pile",
  "version": "0.1.0",
  "description": "Blog Implemented using react",
  "scripts": {
    "start": "npm-run-all --parallel webpack:watch lint:watch",
    "webpack:watch": "webpack -w --config webpack.config.dev.js",
    "webpack:pro": "webpack --config webpack.config.pro.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "license": "MIT",
  "dependencies": {
    "@blueprintjs/core": "^1.4.0",
    "@blueprintjs/table": "^1.2.0",
    "babel-polyfill": "^6.20.0",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-watch": "^2.1.14",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "nock": "^9.0.2",
    "npm-run-all": "^3.1.2",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.4.1",
    "redux-immutable-state-invariant": "^1.2.4",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.5.4",
